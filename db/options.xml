			<control id="fine" type="value" name="Image Lock Fine (Clock Phase)" address="0x3e"/>
		</subgroup>
		<subgroup name="Corners purity">
			<!--- vendor specific controls -->
			<control id="tlpurity" type="value" name="Top left screen purity" address="0xE8"/>
